#!/Users/ariticas/ColdSprings/coldspring-w/coldspring3/coldspring3/env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'divio-cli==3.5.0','console_scripts','divio'
__requires__ = 'divio-cli==3.5.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('divio-cli==3.5.0', 'console_scripts', 'divio')()
    )
