#!/Users/ariticas/ColdSprings/coldspring-w/coldspring3/coldspring3/env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'aldryn-client==2.3.5','console_scripts','aldryn'
__requires__ = 'aldryn-client==2.3.5'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('aldryn-client==2.3.5', 'console_scripts', 'aldryn')()
    )
